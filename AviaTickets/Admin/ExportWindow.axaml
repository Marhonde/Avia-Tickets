<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="AviaTickets.ExportWindow"
        Title="Выгрузка в Excel"
        Width="400" Height="300"
        WindowStartupLocation="CenterScreen">
    <StackPanel Margin="20" Spacing="10">
        <Button Content="Выгрузить все билеты" Click="OnExportAllTicketsClick"/>
        <Button Content="Выгрузить непроданные билеты" Click="OnExportUnsoldTicketsClick"/>
        <Button Content="Выгрузить проданные билеты" Click="OnExportSoldTicketsClick"/>
    </StackPanel>
</Window>
